<%- include("partials/header.ejs") %> 

<h1></h1>
<p> <%= startingContent %> </p> 

<% posts.forEach(function(post){ %> 

    <h2> <%= post.title %> </h2>
    <img src="<%= post.image %>" alt="" />
    <p> <%= post.content.substring(0, 100) + "..." %>
    <a href="/posts/<%= post.title %>">Read more</a>
    </p>

<% }) %>

<% console.log(posts) %> 

